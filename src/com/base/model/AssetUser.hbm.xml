<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.base.model.AssetUser" table="asset_user" lazy="true">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>

        <property name="userName" column="userName" type="string"/>
        <property name="userEmail" column="userEmail" type="string"/>
        <property name="userMobile" column="userMobile" type="string"/>
        <property name="userTel" column="userTel" type="string"/>
        <property name="userDepart" column="userDepart" type="string"/>
     
    </class>
</hibernate-mapping>