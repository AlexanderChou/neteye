<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.base.model.Image" table="image">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="imageName" column="imagename" type="string"/>
        <property name="imageInfo" column="imageinfo" type="string"/>
        <property name="imageType" column="imagetype" type="string"></property>
    </class>
</hibernate-mapping>
