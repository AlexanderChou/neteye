<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.base.model.Service" table="service">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="fileName" column="filename" type="string"/>
        <property name="serviceType" column="servicetype" type="string"/>
        <property name="serviceIP" column="serviceip" type="string"/>
        <property name="serviceMethod" column="servicemethod" type="string"/>
   </class>
</hibernate-mapping>