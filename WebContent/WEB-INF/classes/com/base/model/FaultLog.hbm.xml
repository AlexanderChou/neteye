<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.base.model.FaultLog" table="faultlog">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="beginTime" column="begintime" type="string"/>
        <property name="faultIP" column="faultip" type="string"/>
        <property name="recoverTime" column="recovertime" type="string"/>
   </class>
</hibernate-mapping>